
#include<bits/stdc++.h>
using namespace std;
int main(){
     int ar[4],count=0;
    for(int i=0;i<4;i++){
            cin>>ar[i];
    }
    sort(ar,ar+4);//sorting bcz to compare with
    //following index.ex:1232//if not use ,it will give
    //an error on taste case 5
    for(int i=0;i<3;i++){
            if(ar[i]==ar[i+1]){
                count++;
            }
    }

    cout<<count;
}
